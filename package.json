{
    "name": "@hug/ngx-schematics-utilities",
    "version": "1.0.3",
    "description": "Useful utilities for Angular Schematics",
    "homepage": "https://github.com/DSI-HUG/ngx-schematics-utilities",
    "license": "GPL-3.0-only",
    "author": "HUG - Hôpitaux Universitaires Genève",
    "contributors": [
        "badisi (https://github.com/badisi)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/DSI-HUG/ngx-schematics-utilities.git"
    },
    "keywords": [
        "schematics",
        "utilities",
        "utils",
        "tools",
        "helpers",
        "devkit",
        "angular",
        "Angular CLI",
        "Angular DevKit",
        "sdk",
        "blueprints",
        "scaffolding",
        "template",
        "tooling",
        "code generation"
    ],
    "main": "src/index.js",
    "types": "src/index.d.ts",
    "scripts": {
        "start:docs": "npm run start --prefix=projects/docs",
        "build:docs": "npm run build --prefix=projects/docs",
        "build:lib": "node ./build",
        "test": "ts-node --project=projects/specs/tsconfig.json --require=tsconfig-paths/register node_modules/.bin/jasmine --config=jasmine.json",
        "lint": "eslint projects/lib projects/specs"
    },
    "peerDependencies": {
        "@angular-devkit/core": ">= 11",
        "@angular-devkit/schematics": ">= 11",
        "@angular/cli": ">= 11",
        "@schematics/angular": ">= 11"
    },
    "dependencies": {
        "@badisi/latest-version": "^2.1.1",
        "colors": "1.4.0",
