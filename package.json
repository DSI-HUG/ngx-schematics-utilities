{
    "name": "@hug/ngx-schematics-utilities",
    "version": "4.0.1",
    "description": "Useful utilities for Angular Schematics",
    "homepage": "https://github.com/DSI-HUG/ngx-schematics-utilities",
    "license": "GPL-3.0-only",
    "author": "HUG - Hôpitaux Universitaires Genève",
    "contributors": [
        "badisi (https://github.com/badisi)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/DSI-HUG/ngx-schematics-utilities.git"
    },
    "keywords": [
        "schematics",
        "utilities",
        "utils",
        "tools",
        "helpers",
        "devkit",
        "angular",
        "Angular CLI",
        "Angular DevKit",
        "sdk",
        "blueprints",
        "scaffolding",
        "template",
        "tooling",
        "code generation"
    ],
    "main": "src/index.js",
    "types": "src/index.d.ts",
    "exports": {
        ".": {
            "require": "./src/index.js"
        }
    },
    "scripts": {
        "start:docs": "npm run start --prefix=projects/docs",
        "build:docs": "npm run build --prefix=projects/docs",
        "build:lib": "node ./build.mjs",
        "test": "ts-node --project=projects/specs/tsconfig.json --require=tsconfig-paths/register ./node_modules/jasmine/bin/jasmine.js --config=jasmine.json",
        "lint": "eslint projects/lib projects/specs"
    },
    "peerDependencies": {
        "@angular-devkit/core": ">= 11 < 14",
        "@angular-devkit/schematics": ">= 11 < 14",
        "@angular/cli": ">= 11 < 14",
        "@schematics/angular": ">= 11 < 14"
    },
    "dependencies": {
        "@badisi/latest-version": "^3.0.0",
        "@colors/colors": "^1.5.0",
        "ora": "5.4.1",
        "semver": "^7.3.7"
    },
    "devDependencies": {
        "@hug/eslint-config": "^7.0.2",
        "@types/jasmine": "^4.0.3",
        "@types/node": "^17.0.31",
        "@types/semver": "^7.3.9",
        "cpy": "^9.0.1",
        "eslint": "^8.15.0",
        "jasmine": "^4.1.0",
        "jasmine-spec-reporter": "^7.0.0",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.6.4"
    }
}
