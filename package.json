        "semver": "^7.3.5"
    },
    "devDependencies": {
        "@hug/eslint-config": "^4.0.1",
        "@types/jasmine": "^3.10.2",
        "@types/node": "^17.0.10",
        "@types/semver": "^7.3.9",
        "cpy": "^8.1.2",
        "eslint": "^8.7.0",
        "jasmine": "^4.0.2",
        "jasmine-spec-reporter": "^7.0.0",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.12.0",
        "typescript": "^4.5.5"
    }
